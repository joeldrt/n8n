# URL del túnel ngrok (actualizar cada vez que reinicies ngrok)
WEBHOOK_URL=https://tu-url-ngrok.ngrok.io

# Configuración de PostgreSQL
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n_password

# Configuración de autenticación básica para n8n (no funciona vía variables de entorno)
# El usuario admin se configura en el primer acceso a la interfaz web

# Zona horaria
GENERIC_TIMEZONE=America/Mexico_City